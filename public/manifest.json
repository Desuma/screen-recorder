{
    "name": "Screen Recorder",
    "version": "0.0.1",
    "description": "Screen Recorder.",
    "manifest_version": 3,
    "icons": {
        "16": "public/logo16.png",
        "32": "public/logo32.png",
        "64": "public/logo64.png",
        "128": "public/logo128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "index.js"
            ],
            "run_at": "document_end"
        }
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}